{
  "name": "elite-performance-app",
  "version": "0.1.0",
  "private": true,
  "pnpm": {
    "overrides": {
      "drizzle-orm": "0.44.7",
      "drizzle-kit>drizzle-orm": "0.44.7"
    }
  },
  "scripts": {
    "seed:exercises": "tsx --env-file=.env.local src/seed/seed-exercises.ts",
    "dev:next": "next dev -p 3000",
    "dev:studio": "drizzle-kit studio --port=4983",
    "dev:kill": "kill -9 $(lsof -t -i :3000 -i :4983) || true",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "seed": "curl -X GET http://localhost:3000/api/exercises/seed",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:studio": "DATABASE_URL=postgresql://localhost/elite_performance npx drizzle-kit studio --port 0",
    "db:test": "tsx ./src/db/test.ts",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "knip": "knip --reporter markdown",
    "tsprune": "ts-prune -p tsconfig.json",
    "depcheck": "depcheck --skip-missing=true",
    "circulars": "madge --circular --extensions ts,tsx src",
    "orphans": "madge --orphans --extensions ts,tsx src",
    "graph": "madge --image graph.svg --extensions ts,tsx src",
    "analyze": "ANALYZE=true next build --webpack",
    "push": "drizzle-kit push",
    "size": "size-limit",
    "audit:run": "pnpm typecheck && pnpm lint && pnpm knip && pnpm tsprune && pnpm depcheck && pnpm circulars && pnpm orphans && pnpm analyze && pnpm size"
  },
  "size-limit": [
    {
      "path": "./.next/static/chunks/*.js",
      "limit": "170 KB"
    }
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.90.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "0.44.7",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "next-runtime-env": "^3.3.0",
    "papaparse": "^5.5.3",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@size-limit/preset-app": "^11.2.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^24.10.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "19.2.2",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "drizzle-kit": "^0.31.6",
    "eslint": "9.38.0",
    "eslint-config-next": "15.5.6",
    "eslint-plugin-unused-imports": "^4.3.0",
    "knip": "^5.67.1",
    "madge": "^8.0.0",
    "size-limit": "^11.2.0",
    "tailwindcss": "^4.1.15",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "unimported": "^1.31.1"
  }
}
